{
    "name": "my-fullstack-app",
    "private": true,
    "scripts": {
        "start": "concurrently \"npm run start-react\" \"npm run start-backend\"",
        "start-react": "cd frontend && npm run dev",
        "start-backend": "cd backend && python3 -m uvicorn main:app --reload",
        "install-all": "npm install --prefix frontend && npm install --prefix backend"
    },
    "devDependencies": {
        "concurrently": "^9.1.2"
    },
    "dependencies": {
        "axios": "^1.8.2"
    }
}
